{
    "manifest_version": 2,
    "name": "Gerador de Conteúdo Dinâmico",
    "description": "Essa extensão simples te ajuda a criar conteúdos dinâmicos sem precisar editar o documento JSON!",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://*/*/*/*/templates/builder/"],
            "all_frames": true,
            "js": ["main.js"]            
        }
    ], 
    "browser_action": {
        "default_popup": "popup/index.html",
        "default_title": "Gerador de Conteúdo Dinâmico"
    }
}